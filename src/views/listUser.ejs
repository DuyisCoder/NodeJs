<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/home.css">
    <title>List User</title>
</head>
<style>
    #form-add-user{
        display: none;
    }
</style>
<body>
    <div class="topnav">
        <a  href="/">Home</a>
        <a href="/create-user">Create User</a>
        <a href="/info-user"  class="active">List User</a>
        <a href="#about">About</a>
      </div>
      <h1 style="padding: 0 20px;">List User :
        <!-- <%= JSON.stringify(listUser)  %> -->
    </h1>
      <div class="table-user">
        <table>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>Name</th>
                <th>City</th>
                <th>Action</th>
            </tr>
            <% for(let i=0 ;i<listUser.length;i++){ %>
            <tr>
                <td><%= listUser[i].id %></td>
                <td><%= listUser[i].email %></td>
                <td><%= listUser[i].name %></td>
                <td><%= listUser[i].city %></td>
                <td>
                    <a href="/update-user/<%= listUser[i].id %>">Edit</a>
                    <button>Delete</button>
                </td>
            </tr>
            <% } %>
            
        </table>
      </div>

   
</body>
      
</body>

</html>